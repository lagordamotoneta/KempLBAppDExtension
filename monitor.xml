<monitor>
	<name>Kemp Stats</name>
	<type>managed</type>
	<description>Monitors Kemp Statistics</description>
        <monitor-configuration></monitor-configuration>
        <monitor-run-task>
		<execution-style>periodic</execution-style>
		<execution-frequency-in-seconds>60</execution-frequency-in-seconds>
		<execution-timeout-in-secs>50</execution-timeout-in-secs>
		<name>Run Monitor</name>
           	<type>executable</type>
           	<task-arguments></task-arguments>
            	<executable-task>
                	<type>file</type>
                	<!--  Use only one file element per os-type. -->
                 	<file os-type="windows">runMonitor.bat</file>
           	</executable-task>
         </monitor-run-task>
</monitor>
